          &quot;auction_id&quot;: 1,
          &quot;amount&quot;: 240,
          &quot;created_at&quot;: &quot;2016-01-06T16:59:55+00:00&quot;,
          &quot;updated_at&quot;: &quot;2016-01-06T16:59:55+00:00&quot;,
          &quot;id&quot;: 67,
          &quot;bidder&quot;: {
            &quot;github_id&quot;: null,
